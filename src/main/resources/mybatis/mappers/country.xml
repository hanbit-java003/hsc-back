<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="country">

	<select id="selectCountry" parameterType="string" resultType="countryVO">
		SELECT id, name, title, text, flag
		  FROM tbl_country
		 WHERE id = #{id}
	</select>
	
</mapper>









